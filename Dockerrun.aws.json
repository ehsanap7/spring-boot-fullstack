{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "ehsanap-react",
      "image": "ehsanap/ehsanap-react:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5173
        }
      ]
    },
    {
      "name": "ehsanap-api",
      "image": "ehsanap/ehsanap-api:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://TODO:5432/customer"
        }
      ]
    }
  ]
}